functuion [TrimmedTrace]=LinkAllTrimmedTrace()
clearvars -except Trace_Trimmed_all;
filePath=dir('Y:\data\20190705\*.mat');
Trace_Trimmed_all={};
maxTau=15;
Fps=20;
Lens=20;
for ii=38:40 % i represent the index in 'dir'
    dbstop if error;
    load([filePath(ii).folder,'\',filePath(ii).name]);
    [Trace_Trimmed] = TraceTrimmer(Trace_All, maxTau*Fps);
    Trace_Trimmed_all=[Trace_Trimmed_all,Trace_Trimmed];
end



end
